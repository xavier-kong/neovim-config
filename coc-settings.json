{
    "coc.preferences.formatOnSaveFiletypes": ["json", "javascriptreact", "go"],
    "eslint.filetypes": ["javascript", "typescript", "typescriptreact", "javascriptreact", "typescript.tsx"],
    "coc.preferences.diagnostic.virtualText": true,
    "prettier.disableLanguages": ["javascript", "typescript"],
      "languageserver": {
        "golang": {
          "command": "gopls",
          "rootPatterns": ["go.mod"],
          "filetypes": ["go"]
